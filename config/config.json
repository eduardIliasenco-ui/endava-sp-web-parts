{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "e-link-block-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/eLinkBlock/ELinkBlockWebPart.js",
          "manifest": "./src/webparts/eLinkBlock/ELinkBlockWebPart.manifest.json"
        }
      ]
    },
    "e-tile-block-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/eTileBlock/ETileBlockWebPart.js",
          "manifest": "./src/webparts/eTileBlock/ETileBlockWebPart.manifest.json"
        }
      ]
    },
    "hero-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hero/HeroWebPart.js",
          "manifest": "./src/webparts/hero/HeroWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ELinkBlockWebPartStrings": "lib/webparts/eLinkBlock/loc/{locale}.js",
    "ETileBlockWebPartStrings": "lib/webparts/eTileBlock/loc/{locale}.js",
    "HeroWebPartStrings": "lib/webparts/hero/loc/{locale}.js"
  }
}
